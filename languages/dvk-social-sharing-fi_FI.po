msgid ""
"Automatically adds the sharing links to the end of the selected post types."
msgstr "Automaattisesti lisää jakolinkit valittujen sisältöjen loppuun."

#: .././includes/views/settings-page.php:54
msgid "Load Icon CSS?"
msgstr "Lataa ikonien CSS?"

#: .././includes/views/settings-page.php:57
#: .././includes/views/settings-page.php:81
msgid "Yes"
msgstr "Kyllä"

#: .././includes/views/settings-page.php:58
#: .././includes/views/settings-page.php:82
msgid "No"
msgstr "Ei"

#: .././includes/views/settings-page.php:60
msgid "Adds simple but pretty icons to the sharing links."
msgstr "Lisää yksinkertaiset, mutta kauniit ikonit jakolinkkeihin."

#: .././includes/views/settings-page.php:66
msgid "Icon Size"
msgstr "Ikonin koko"

#: .././includes/views/settings-page.php:70
msgid "Small"
msgstr "Pieni"

#: .././includes/views/settings-page.php:70
#: .././includes/views/settings-page.php:71
msgid "pixels"
msgstr "pikseliä"

#: .././includes/views/settings-page.php:71
